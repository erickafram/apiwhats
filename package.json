{
  "name": "whatsapp-chatbot-system",
  "version": "1.0.0",
  "description": "Sistema completo de chatbot para WhatsApp com IA e editor visual de fluxos",
  "main": "server.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "setup:frontend": "node scripts/setup-frontend.js",
    "restart": "node scripts/restart-server.js",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:minimal": "node scripts/test-minimal.js",
    "test:basic": "node scripts/test-basic.js",
    "test:flows": "node scripts/test-flows.js",
    "test:phase4": "node scripts/test-phase4.js",
    "test:system": "node scripts/test-system.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:demo": "npx sequelize-cli db:seed --seed 20241229000001-demo-data.js",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "ai",
    "baileys",
    "together.xyz",
    "react",
    "node.js"
  ],
  "author": "WhatsApp Chatbot System",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.4.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
